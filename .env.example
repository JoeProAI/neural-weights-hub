# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id

# Firebase Admin SDK (Server-side)
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_CLIENT_EMAIL=your_service_account@your_project.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_private_key_here\n-----END PRIVATE KEY-----"

# Daytona API Configuration
DAYTONA_API_KEY=your_daytona_api_key
DAYTONA_ORG_ID=your_daytona_org_id
DAYTONA_SERVER_URL=https://app.daytona.io/api
DAYTONA_WORKSPACE_ID=your_workspace_id

# Model Volume IDs (Pre-created in Daytona)
DAYTONA_GPT_20B_VOLUME_ID=your_20b_volume_id
DAYTONA_GPT_120B_VOLUME_ID=your_120b_volume_id

# Production Server IDs (Optional - for protected servers)
DAYTONA_GPT_20B_SANDBOX_ID=your_20b_server_id
DAYTONA_GPT_120B_SANDBOX_ID=your_120b_server_id

# Modal API Endpoints (for AI inference)
MODAL_GPT_20B_ENDPOINT=https://your-modal-endpoint-20b.modal.run
MODAL_GPT_120B_ENDPOINT=https://your-modal-endpoint-120b.modal.run

# Stripe Configuration (for billing)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Environment
NODE_ENV=development
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret
